{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": false,
        "no-var-requires": true,
        "quotemark": false,
        "ordered-imports": false,
        "interface-name": false,
        "trailing-comma": false,
        "no-trailing-whitespace": false,
        "no-empty": false,
        "only-arrow-functions": false
    },
    "rulesDirectory": []
}